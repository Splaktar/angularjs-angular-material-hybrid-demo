{
  "name": "angularjs-angular-material-hybrid-demo",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "npm run build:templates && ng serve",
    "build:templates": "gulp templates",
    "watch:templates": "gulp watch",
    "build": "npm run build:templates && ng build",
    "test": "ng test",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "smex": "source-map-explorer",
    "deploy": "npm run build:templates && ng deploy",
    "update": "npm-check -u"
  },
  "dependencies": {
    "@angular/animations": "10.0.1",
    "@angular/cdk": "10.0.0",
    "@angular/common": "10.0.1",
    "@angular/compiler": "10.0.1",
    "@angular/core": "10.0.1",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "10.0.1",
    "@angular/material": "10.0.0",
    "@angular/platform-browser": "10.0.1",
    "@angular/platform-browser-dynamic": "10.0.1",
    "@angular/upgrade": "10.0.1",
    "@schuchard/prettier": "^3.1.0",
    "@types/angular": "^1.7.2",
    "@types/angular-material": "^1.1.71",
    "angular": "^1.8.0",
    "angular-animate": "^1.8.0",
    "angular-aria": "^1.8.0",
    "angular-material": "^1.1.23",
    "angular-messages": "^1.8.0",
    "firebase": "^7.15.5",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1000.0",
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.1",
    "@angular/language-service": "~10.0.1",
    "@types/gulp": "^4.0.6",
    "@types/node": "^14.0.14",
    "codelyzer": "^6.0.0-next.1",
    "firebase-tools": "^8.4.3",
    "gulp": "^4.0.2",
    "gulp-angular-templatecache": "^3.0.0",
    "gulp-htmlmin": "^5.0.1",
    "husky": "4.2.5",
    "lint-staged": "^10.2.11",
    "npm-check": "^5.9.2",
    "prettier": "2.0.5",
    "rxjs-tslint-rules": "^4.33.3",
    "source-map-explorer": "^2.4.2",
    "ts-node": "^8.10.2",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "1.18.0",
    "tsutils": "^3.17.1",
    "typescript": "~3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && ng lint"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  }
}
