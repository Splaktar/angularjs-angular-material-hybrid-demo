{
  "name": "angularjs-angular-material-hybrid-demo",
  "version": "0.4.1",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "npm run build:templates && ng serve",
    "start:static": "firebase serve --only hosting:app",
    "build:templates": "gulp templates",
    "watch:templates": "gulp watch",
    "build": "npm run build:templates && ng build",
    "build:prod": "npm run build:templates && ng build --prod",
    "test": "ng test",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "smex": "source-map-explorer",
    "deploy": "npm run build:templates && ng deploy",
    "update": "npm-check -u"
  },
  "dependencies": {
    "@angular/animations": "11.0.0",
    "@angular/cdk": "11.0.0",
    "@angular/common": "11.0.0",
    "@angular/compiler": "11.0.0",
    "@angular/core": "11.0.0",
    "@angular/fire": "^6.0.5",
    "@angular/forms": "11.0.0",
    "@angular/material": "11.0.0",
    "@angular/platform-browser": "11.0.0",
    "@angular/platform-browser-dynamic": "11.0.0",
    "@angular/upgrade": "11.0.0",
    "@schuchard/prettier": "^3.1.0",
    "@types/angular": "^1.8.0",
    "@types/angular-material": "^1.1.72",
    "angular": "^1.8.2",
    "angular-animate": "^1.8.2",
    "angular-aria": "^1.8.2",
    "angular-material": "^1.2.1",
    "angular-messages": "^1.8.2",
    "angular-sanitize": "^1.8.2",
    "firebase": "^8.0.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.0",
    "@angular/cli": "~11.0.0",
    "@angular/compiler-cli": "~11.0.0",
    "@angular/language-service": "~11.0.0",
    "@types/gulp": "^4.0.7",
    "@types/node": "^14.14.7",
    "codelyzer": "^6.0.1",
    "debug": "^4.1.1",
    "firebase-tools": "^8.16.0",
    "gulp": "~4.0.2",
    "gulp-angular-templatecache": "~3.0.0",
    "gulp-htmlmin": "~5.0.1",
    "husky": "4.3.0",
    "lint-staged": "^10.5.1",
    "npm-check": "^5.9.2",
    "prettier": "2.1.2",
    "rxjs-tslint-rules": "^4.34.5",
    "source-map-explorer": "^2.5.0",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tsutils": "^3.17.1",
    "typescript": "~4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && ng lint"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  }
}
